<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<f:Form editable="true">
		<f:layout>
			<f:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanM="0"
				emptySpanS="0" emptySpanL="0" emptySpanXL="0" columnsXL="3" columnsL="3" columnsM="1" singleContainerFullSize="true"/>
		</f:layout>
		<f:formContainers>
			<f:FormContainer title="{i18n>CURRENT_WAGE}">
				<f:formElements>
					<f:FormElement label="{i18n>Crslr}">
						<f:fields>
							<Input type="Number" value="{path: 'candidateProcessModel>/CandidateWage/Crslr', type:'sap.ui.model.odata.type.Decimal'}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}"/>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Waers' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/CrslrWaers}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Key}"/>
							</ComboBox>
							<RadioButtonGroup columns="2" buttons="{path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Payty' }]}">
								<RadioButton text="{Text}" selected="{= ${Key} === ${candidateProcessModel>/CandidateWage/CrslrPayty} }" select="onPaytySelected"
									visible="{= ${Key} !== ''}" cd:targetField="/CandidateWage/CrslrPayty" cd:targetValue="{Key}"
									editable="{candidateProcessModel>/ViewSettings/WageInit}"></RadioButton>
							</RadioButtonGroup>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>CrslrAdben}">
						<f:fields>
							<TextArea value="{candidateProcessModel>/CandidateWage/CrslrAdben}" maxLength="255"
								editable="{candidateProcessModel>/ViewSettings/WageInit}" rows="3"/>
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
			<f:FormContainer title="{i18n>EXPECTED_WAGE}">
				<f:formElements>
					<f:FormElement label="{i18n>Exslr}">
						<f:fields>
							<Input type="Number" value="{path: 'candidateProcessModel>/CandidateWage/Exslr', type:'sap.ui.model.odata.type.Decimal'}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}"/>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Waers' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/ExslrWaers}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Key}"/>
							</ComboBox>
							<RadioButtonGroup columns="2" buttons="{path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Payty' }]}">
								<RadioButton text="{Text}" selected="{= ${Key} === ${candidateProcessModel>/CandidateWage/ExslrPayty} }" select="onPaytySelected"
									visible="{= ${Key} !== ''}" cd:targetField="/CandidateWage/ExslrPayty" cd:targetValue="{Key}"
									editable="{candidateProcessModel>/ViewSettings/WageInit}"></RadioButton>
							</RadioButtonGroup>
						</f:fields>
					</f:FormElement>
					<!--<f:FormElement label="{i18n>WAGE_TYPE}">-->
					<!--	<f:fields>-->
					<!--		<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Payty' }]}"-->
					<!--			filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/ExslrPayty}"-->
					<!--			editable="{candidateProcessModel>/ViewSettings/WageInit}">-->
					<!--			<core:ListItem key="{Key}" text="{Text}"/>-->
					<!--		</ComboBox>-->
					<!--	</f:fields>-->
					<!--</f:FormElement>-->
					<!--<f:FormElement label="{i18n>WAGE_PERIOD}">-->
					<!--	<f:fields>-->
					<!--		<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Calty' }]}"-->
					<!--			filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/ExslrCalty}"-->
					<!--			editable="{candidateProcessModel>/ViewSettings/WageInit}">-->
					<!--			<core:ListItem key="{Key}" text="{Text}"/>-->
					<!--		</ComboBox>-->
					<!--	</f:fields>-->
					<!--</f:FormElement>-->
					<f:FormElement label="{i18n>ExslrAdben}">
						<f:fields>
							<TextArea value="{candidateProcessModel>/CandidateWage/ExslrAdben}" maxLength="255"
								editable="{candidateProcessModel>/ViewSettings/WageInit}" rows="3"/>
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
			<f:FormContainer title="{i18n>WAGE_DETERMINATION_PARAMETERS}">
				<f:formElements>
					<f:FormElement label="{i18n>SECTOR_EXPERIENCE}">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Scexp' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/Scexp}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Text}"/>
							</ComboBox>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>ENGLISH_LEVEL}">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Enlvl' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/Enlvl}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Text}"/>
							</ComboBox>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>UNIVERSITY_TYPE}">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Trgun' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/Trgun}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Text}"/>
							</ComboBox>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>EDUCATION_LEVEL}">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Slart' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/Slart}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Text}"/>
							</ComboBox>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>IS_ENGINEER}">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Iseng' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/Iseng}"
								editable="{candidateProcessModel>/ViewSettings/WageInit}">
								<core:ListItem key="{Key}" text="{Text}"/>
							</ComboBox>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>POSITION_TYPE}" visible="{= !${candidateProcessModel>/ViewSettings/WageInit} }">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Poscl' }]}"
								filterSecondaryValues="true" selectedKey="{candidateProcessModel>/CandidateWage/Poscl}"
								editable="{candidateProcessModel>/ViewSettings/WageOffer}">
								<core:ListItem key="{Key}" text="{Text}"/>
							</ComboBox>
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
		</f:formContainers>
	</f:Form>
</core:FragmentDefinition>