<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog title="{i18n>JOB_SELECTION}" class="sapUiPopupWithPadding"
		items="{ path:'GenericServices>/ObjectSearchSet', filters: [ { path : 'Otype', operator : 'EQ', value1 : 'C'} ]}" search="onJobSearch"
		confirm="onJobSelect" contentWidth="{= ${device>/system/phone} ? '100%' : '60%'}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{GenericServices>Stext}"/>
				<Text text="{GenericServices>Objid}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="{i18n>JOB_NAME}"/>
				</header>
			</Column>
			<Column width="10rem">
				<header>
					<Text text="{i18n>JOB_OBJECT_ID}"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>