								<TimePicker editable="false"
									value="{path: 'candidateProcessModel>/CandidateTravelFormHR/Plart', type: 'sap.ui.model.odata.type.Time', formatOptions:{pattern:'HH:mm'}}"
									displayFormat="HH:mm"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer title="Transfer Formu" visible="{candidateProcessModel>/CandidateTransferFormHR/Exist}">
