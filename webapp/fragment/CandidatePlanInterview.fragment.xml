									<layoutData>
										<l:GridData span="XL1 L1 M1 S3"/>
									</layoutData>
								</Button>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>INTERVIEW_DATE}" visible="{= !${candidateProcessModel>/CandidateInterviewPlan/PlanHr}}">
							<f:fields>
								<DatePicker id="idPlannedInterviewDateDp" placeholder="{i18n>ENTER_HR_INTERVIEW_DATE}"
									value="{path:'candidateProcessModel>/CandidateInterviewPlan/IntdtDp', type: 'sap.ui.model.type.Date', formatOptions:{ UTC: true, pattern: 'dd.MM.yyyy'}}"
									minDate="{path: 'candidateProcessModel>/CandidateInterviewPlan/IntdtHr', formatter:'.formatter.setRestrictPastDate'}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>INTERVIEW_TIME}" visible="{= !${candidateProcessModel>/CandidateInterviewPlan/PlanHr}}">
							<f:fields>
								<TimePicker id="idPlannedInterviewTimeDp" placeholder="{i18n>ENTER_HR_INTERVIEW_TIME}"
