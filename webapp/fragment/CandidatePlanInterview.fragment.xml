													<Text text="{candidateProcessModel>Inpnm}" class="sapUiSmallMarginBegin"/>
												</HBox>
												<Button type="Reject" icon="sap-icon://delete" press="onDeleteParticipant" cd:referencePernr="{candidateProcessModel>Inper}"
													cd:sourcePath="/CandidateInterviewPlan/ParticipantsHr"/>
											</cells>
										</ColumnListItem>
									</items>
									<layoutData>
										<l:GridData span="XL12 L12 M12 S12"/>
									</layoutData>
								</Table>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer
