<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<FlexBox direction="Column" width="100%">
		<f:Form editable="true" id="idCandidateInterviewPlanForm">
			<f:layout>
				<f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false"
					emptySpanXL="{= ${candidateProcessModel>/CandidateInterviewPlan/PlanHr} &amp;&amp; ${candidateProcessModel>/CandidateInterviewPlan/PlanDp} ? 2 : 4 }"
					emptySpanL="{= ${candidateProcessModel>/CandidateInterviewPlan/PlanHr} &amp;&amp; ${candidateProcessModel>/CandidateInterviewPlan/PlanDp} ? 2 : 4 }"
					emptySpanM="{= ${candidateProcessModel>/CandidateInterviewPlan/PlanHr} &amp;&amp; ${candidateProcessModel>/CandidateInterviewPlan/PlanDp} ? 1 : 3 }"
					emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1"/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer title="{i18n>PLAN_FIRST_INTERVIEW}" visible="{candidateProcessModel>/CandidateInterviewPlan/PlanHr}">
					<f:formElements>
						<f:FormElement label="{i18n>INTERVIEW_RESPONSIBLE}">
							<f:fields>
								<Input
									value="{ parts: [{ path: 'candidateProcessModel>/CandidateInterviewPlan/IntvwHr' }, { path: 'candidateProcessModel>/CandidateInterviewPlan/IntvnHr'}], formatter: '.formatter.convertZeroObjectNumber'}"
									valueHelpOnly="true" valueHelpRequest="onEmployeeValueRequest" showValueHelp="true" cd:sourceField="IntvwHr"/>
								<Button icon="sap-icon://reset" press="onResetEmployee" type="Reject" tooltip="{i18n>RESET_FIELD}" cd:sourceField="IntvwHr">
									<layoutData>
										<l:GridData span="XL1 L1 M1 S3"/>
									</layoutData>
								</Button>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>INTERVIEW_DATE}">
							<f:fields>
								<DatePicker id="idPlannedInterviewDateHr" placeholder="{i18n>ENTER_HR_INTERVIEW_DATE}"
									value="{path:'candidateProcessModel>/CandidateInterviewPlan/IntdtHr', type: 'sap.ui.model.type.Date', formatOptions:{ UTC: true, pattern: 'dd.MM.yyyy'}}"
									minDate="{path: 'candidateProcessModel>/CandidateInterviewPlan/IntdtHr', formatter:'.formatter.setRestrictPastDate'}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="{i18n>INTERVIEW_TIME}">
							<f:fields>
								<TimePicker id="idPlannedInterviewTimeHr" placeholder="{i18n>ENTER_HR_INTERVIEW_TIME}"
									value="{path:'candidateProcessModel>/CandidateInterviewPlan/InttmHr', type: 'sap.ui.model.odata.type.Time', formatOptions:{pattern:'HH:mm'}}"
									displayFormat="HH:mm"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer title="{i18n>PLAN_SECOND_INTERVIEW}"
					visible="{= ${candidateProcessModel>/CandidateInterviewPlan/PlanDp} || ${candidateProcessModel>/CandidateInterviewPlan/Inttg} }">
					<f:formElements>
						<f:FormElement label="{i18n>INTERVIEW_RESPONSIBLE}">
							<f:fields>
								<Input
									value="{ parts: [{ path: 'candidateProcessModel>/CandidateInterviewPlan/IntvwDp' }, { path: 'candidateProcessModel>/CandidateInterviewPlan/IntvnDp'}], formatter: '.formatter.convertZeroObjectNumber'}"
									valueHelpOnly="true" valueHelpRequest="onEmployeeValueRequest" showValueHelp="true" cd:sourceField="IntvwDp"/>
