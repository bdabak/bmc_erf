											</content>
										</Toolbar>
									</headerToolbar>
									<columns>
										<Column>
											<Text text="{i18n>PARTICIPANT_NAME}"/>
										</Column>
										<Column hAlign="Center" demandPopin="true" minScreenWidth="Tablet" popinDisplay="WithoutHeader">
											<Text text="{i18n>ACTION_LIST}"/>
										</Column>
									</columns>
