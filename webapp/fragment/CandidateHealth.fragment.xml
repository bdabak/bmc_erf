								selectedKey="{candidateModel>/candidateData/CandidateHealth/Prmdc}" change="onChangeHealthArea" cd:area="Prmde">
								<core:Item key="{Key}" text="{Text}"></core:Item>
							</ComboBox>
						</f:fields>
