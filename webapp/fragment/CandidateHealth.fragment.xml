							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Medrp' } ] }"
								selectedKey="{candidateModel>/candidateData/CandidateHealth/Medrp}">
								<core:Item key="{Key}" text="{Text}"></core:Item>
							</ComboBox>
						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>Chdse}">
						<f:fields>
							<ComboBox items="{ path:'/CandidateValueHelpSet' , filters:[{path: 'Help' , operator : 'EQ' , value1 : 'Chdse' } ] }"
								selectedKey="{candidateModel>/candidateData/CandidateHealth/Chdse}" change="onChangeHealthArea" cd:area="Chdsx">
