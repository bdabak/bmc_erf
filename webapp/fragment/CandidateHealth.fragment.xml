						</f:fields>
					</f:FormElement>
					<f:FormElement label="{i18n>Losce}" visible="{= ${candidateModel>/candidateData/CandidateHealth/Loscn} === '1' ? true : false }">
						<f:fields>
							<TextArea value="{candidateModel>/candidateData/CandidateHealth/Losce}"/>
						</f:fields>
					</f:FormElement>
				</f:formElements>
			</f:FormContainer>
		</f:formContainers>
	</f:Form>
</core:FragmentDefinition>